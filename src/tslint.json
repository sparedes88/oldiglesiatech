{
    "extends": "../tslint.json",
    "rules": {
        "variable-name": [true, "allow-snake-case"],
        "no-inferrable-types": false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
